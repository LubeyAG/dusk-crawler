{
    "name": "katsana/dusk-crawler",
    "description": "Web Crawler using Laravel Dusk",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "KATSANA Developer",
            "email": "developer@katsana.com"
        },
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "DuskCrawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DuskCrawler\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "laravel/dusk": "^5.9",
        "orchestra/canvas-core": "^4.7",
        "orchestra/dusk-updater": "^1.2",
        "react/promise": "^2.7",
        "symfony/dom-crawler": "^4.3 || ^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "orchestra/canvas": "^4.5",
        "phpunit/phpunit": "^8.4 || ^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DuskCrawler\\DuskServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
