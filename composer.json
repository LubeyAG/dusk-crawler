{
    "name": "laravie/dusk-crawler",
    "description": "Web Crawler using Laravel Dusk",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Laravie\\DuskCrawler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravie\\DuskCrawler\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "laravel/dusk": "^5.8.2",
        "orchestra/dusk-updater": "^1.2",
        "orchestra/canvas-core": "^4.6",
        "symfony/dom-crawler": "^4.3 || ^5.0"
    },
    "require-dev": {
        "orchestra/canvas": "^4.5",
        "orchestra/testbench": "^4.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravie\\DuskCrawler\\DuskServiceProvider"
            ]
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
